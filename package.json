{
  "name": "nlp-graph",
  "version": "1.0.0",
  "description": "Graphs controlled by natural language",
  "scripts": {
    "build": "gulp build",
    "build:production": "gulp build --production",
    "postinstall": "npm run build:production",
    "start": "cd server && node ./server.js",
    "test": "npm run test:server && npm run test:client",
    "test:client": "karma start ./client/test/karma.conf.js --single-run",
    "test:server": "cd server && mocha ./test/**/*.js --recursive --exit",
    "migrate": "knex migrate:latest --knexfile ./server/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./server/knexfile.js",
    "watch": "gulp"
  },
